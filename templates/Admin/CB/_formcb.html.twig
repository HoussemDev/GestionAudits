{{ form_start(form) }}
<div class="row">
    <div class="col-md-12">{{ form_row(form.number) }}</div>
    <div class="col-md-12">{{ form_row(form.name) }}</div>
    <div class="col-md-12">{{ form_row(form.city) }}</div>
    <div class="col-md-12">{{ form_row(form.adress) }}</div>
    <div class="col-md-12">{{ form_row(form.country) }}</div>
    <div class="col-md-12">{{ form_row(form.contactperson) }}</div>
    <div class="col-md-12">{{ form_row(form.website) }}</div>
</div>


{{ form_widget(form) }}
<button class="btn btn-primary"> {{ button|default('Save') }} </button>
{{ form_end(form) }}