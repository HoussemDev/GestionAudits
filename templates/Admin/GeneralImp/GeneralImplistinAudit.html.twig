{% extends 'base.html.twig' %}
{% block body %}



    <div class="container mt-4">
    {% for message in app.flashes('success') %}
        <div class="alert alert-success">
            {{ message }}
        </div>
    {% endfor %}
    </div>
    <div class="container theme-showcase" role="main">

    <div class="jumbotron">
    <div class="text-right">


        <a href="{{ path('admin_generalimp_new', {title: audits.title}) }}" class="btn btn-primary">Create General impression</a>
     <br>
        <br>



    </div>
    <div class="row">
    <div class="col-sm-4 dropdown-menu-sm-left">
        {{ include('Admin/Audit/auditleftmenu.html.twig') }}
    </div>
    <div class="col-sm-8">

    <div class="container-fluid col-sm-8">
    <table class="table table-hover">
        <thead>
        <tr>
            <th>General Impression Title</th>




        </tr>
        </thead>
        <tbody>
        {% for generalimp in generalimps %}
            <tr>


                <td> <a href="{{ path('genimp_show', {id: generalimp.id, slug: generalimp.slug}) }}" >{{ generalimp.titlegeneralimp}} </a></td>


            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}